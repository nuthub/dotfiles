[locale]
local_timezone= Europe/Berlin
default_timezone= Europe/Berlin
timeformat = %H:%M
dateformat = %Y-%m-%d
longdateformat = %Y-%m-%d %a
datetimeformat = %Y-%m-%d %H:%M
longdatetimeformat = %Y-%m-%d %H:%M

[view]
blank_line_before_day = True

[calendars]

[[nuthouse]]
path = ~/.calendars/nuthouse/*
type = discover
